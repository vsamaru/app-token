    "@bubkoo/eslint-config": "^1.2.0",
    "@bubkoo/semantic-release-config": "^1.4.0",
    "@bubkoo/tsconfig": "^1.0.0",
    "@probot/adapter-aws-lambda-serverless": "^3.0.1",
